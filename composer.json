{
    "name": "jaraba/impact-platform-saas",
    "description": "Plataforma SaaS de impacto para ecosistemas de productores locales",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://jaraba.io",
    "authors": [
        {
            "name": "Ecosistema Jaraba",
            "email": "dev@jaraba.io"
        }
    ],
    "require": {
        "php": ">=8.4",
        "composer/installers": "^2.0",
        "cweagans/composer-patches": "^1.7",
        "defuse/php-encryption": "^2.4",
        "drupal/admin_toolbar": "^3.4",
        "drupal/ai": "^1.2",
        "drupal/ai_provider_anthropic": "^1.2",
        "drupal/ai_provider_google_vertex": "^1.1",
        "drupal/ai_provider_openai": "^1.2",
        "drupal/ai_translate": "^1.3",
        "drupal/backup_migrate": "^5.0",
        "drupal/bpmn_io": "^3.0",
        "drupal/commerce": "^3.1",
        "drupal/commerce_promotion": "^3.0",
        "drupal/commerce_shipping": "^3.1",
        "drupal/commerce_stripe": "^1.2",
        "drupal/components": "^3.0@beta",
        "drupal/config_split": "^2.0",
        "drupal/core-composer-scaffold": "^11.0",
        "drupal/core-project-message": "^11.0",
        "drupal/core-recommended": "^11.0",
        "drupal/ctools": "^4.0",
        "drupal/domain": "^2.0",
        "drupal/domain_access": "^2.0",
        "drupal/eca": "^3.0",
        "drupal/entity": "^1.4",
        "drupal/entity_reference_revisions": "^1.11",
        "drupal/eu_cookie_compliance": "^1.24",
        "drupal/field_group": "^3.4",
        "drupal/gdpr": "^3.0",
        "drupal/geocoder": "^4.30",
        "drupal/geofield": "^10.3",
        "drupal/group": "^3.2",
        "drupal/inline_entity_form": "^3.0@RC",
        "drupal/jsonapi_extras": "^3.24",
        "drupal/leaflet": "^10.4",
        "drupal/metatag": "^2.0",
        "drupal/paragraphs": "^1.17",
        "drupal/pathauto": "^1.12",
        "drupal/redirect": "^1.9",
        "drupal/redis": "^1.7",
        "drupal/search_api": "^1.31",
        "drupal/simple_sitemap": "^4.2",
        "drupal/symfony_mailer": "^1.4",
        "drupal/token": "^1.13",
        "drupal/twig_tweak": "^3.3",
        "drupal/views_bulk_operations": "^4.2",
        "drupal/views_data_export": "^1.4",
        "drupal/webform": "^6.2",
        "drush/drush": "^13",
        "stripe/stripe-php": "^15.0",
        "tecnickcom/tcpdf": "^6.10"
    },
    "require-dev": {
        "drupal/core-dev": "^11.0",
        "drupal/devel": "^5.2",
        "drupal/devel_entity_updates": "^5.0",
        "drupal/restui": "^1.21",
        "mglaman/phpstan-drupal": "^1.2",
        "phpspec/prophecy-phpunit": "^2.0",
        "phpstan/phpstan-deprecation-rules": "^1.1",
        "phpunit/phpunit": "^11.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "php-http/discovery": true,
            "phpstan/extension-installer": true,
            "tbachert/spi": true
        },
        "sort-packages": true,
        "platform": {
            "php": "8.4"
        }
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            },
            "file-mapping": {
                "[web-root]/sites/development.services.yml": false
            }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "patches": {},
        "composer-exit-on-patch-failure": true,
        "enable-patching": true
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "scripts": {
        "post-install-cmd": [
            "@drupal-scaffold"
        ],
        "post-update-cmd": [
            "@drupal-scaffold"
        ],
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "test": "phpunit -c web/modules/custom/ecosistema_jaraba_core/phpunit.xml",
        "phpstan": "phpstan analyse web/modules/custom --level=5",
        "sass": "cd web/modules/custom/ecosistema_jaraba_core && npx sass scss/main.scss css/ecosistema-jaraba-core.css --style=compressed"
    }
}
