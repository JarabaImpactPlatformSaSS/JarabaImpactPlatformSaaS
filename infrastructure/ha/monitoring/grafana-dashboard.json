{
  "__inputs": [
    {
      "name": "DS_PROMETHEUS",
      "label": "Prometheus",
      "description": "",
      "type": "datasource",
      "pluginId": "prometheus",
      "pluginName": "Prometheus"
    }
  ],
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": "-- Grafana --",
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      },
      {
        "datasource": "${DS_PROMETHEUS}",
        "enable": true,
        "expr": "changes(redis_sentinel_master_ok_sentinels[1m]) > 0",
        "iconColor": "red",
        "name": "Redis Failover",
        "tagKeys": "event",
        "titleFormat": "Redis Sentinel Failover"
      }
    ]
  },
  "description": "Jaraba Impact Platform - High Availability Stack Overview (N3 Enterprise Class, Doc 197)",
  "editable": true,
  "gnetId": null,
  "graphTooltip": 1,
  "id": null,
  "links": [],
  "panels": [
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "id": 100,
      "title": "Cluster Health Overview",
      "type": "row"
    },
    {
      "datasource": "${DS_PROMETHEUS}",
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "red", "value": 1 },
              { "color": "yellow", "value": 2 },
              { "color": "green", "value": 3 }
            ]
          },
          "mappings": []
        }
      },
      "gridPos": { "h": 4, "w": 4, "x": 0, "y": 1 },
      "id": 1,
      "options": {
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "center",
        "textMode": "value_and_name",
        "reduceOptions": { "calcs": ["lastNotNull"] }
      },
      "targets": [
        {
          "expr": "mysql_global_status_wsrep_cluster_size",
          "legendFormat": "Galera Nodes"
        }
      ],
      "title": "Galera Cluster Size",
      "type": "stat"
    },
    {
      "datasource": "${DS_PROMETHEUS}",
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "green", "value": 1 }
            ]
          }
        }
      },
      "gridPos": { "h": 4, "w": 4, "x": 4, "y": 1 },
      "id": 2,
      "options": {
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "center",
        "textMode": "value_and_name",
        "reduceOptions": { "calcs": ["lastNotNull"] }
      },
      "targets": [
        {
          "expr": "haproxy_backend_active_servers{backend=\"drupal_servers\"}",
          "legendFormat": "HAProxy Backends"
        }
      ],
      "title": "Active HAProxy Backends",
      "type": "stat"
    },
    {
      "datasource": "${DS_PROMETHEUS}",
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "green", "value": 1 }
            ]
          }
        }
      },
      "gridPos": { "h": 4, "w": 4, "x": 8, "y": 1 },
      "id": 3,
      "options": {
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "center",
        "textMode": "value_and_name",
        "reduceOptions": { "calcs": ["lastNotNull"] }
      },
      "targets": [
        {
          "expr": "redis_up{role=\"master\"}",
          "legendFormat": "Redis Master"
        }
      ],
      "title": "Redis Master Status",
      "type": "stat"
    },
    {
      "datasource": "${DS_PROMETHEUS}",
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 0.5 },
              { "color": "red", "value": 0.8 }
            ]
          }
        }
      },
      "gridPos": { "h": 4, "w": 4, "x": 12, "y": 1 },
      "id": 4,
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "justifyMode": "center",
        "textMode": "value_and_name",
        "reduceOptions": { "calcs": ["lastNotNull"] }
      },
      "targets": [
        {
          "expr": "proxysql_connection_pool_conn_used / proxysql_connection_pool_conn_max",
          "legendFormat": "ProxySQL Pool"
        }
      ],
      "title": "ProxySQL Pool Usage",
      "type": "stat"
    },
    {
      "datasource": "${DS_PROMETHEUS}",
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 0.01 },
              { "color": "red", "value": 0.05 }
            ]
          }
        }
      },
      "gridPos": { "h": 4, "w": 4, "x": 16, "y": 1 },
      "id": 5,
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "justifyMode": "center",
        "textMode": "value_and_name",
        "reduceOptions": { "calcs": ["lastNotNull"] }
      },
      "targets": [
        {
          "expr": "sum(rate(http_requests_total{job=\"drupal\",status=~\"5..\"}[5m])) / sum(rate(http_requests_total{job=\"drupal\"}[5m]))",
          "legendFormat": "Error Rate"
        }
      ],
      "title": "Application Error Rate",
      "type": "stat"
    },
    {
      "datasource": "${DS_PROMETHEUS}",
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 1 },
              { "color": "red", "value": 2 }
            ]
          }
        }
      },
      "gridPos": { "h": 4, "w": 4, "x": 20, "y": 1 },
      "id": 6,
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "justifyMode": "center",
        "textMode": "value_and_name",
        "reduceOptions": { "calcs": ["lastNotNull"] }
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket{job=\"drupal\"}[5m])) by (le))",
          "legendFormat": "P95 Latency"
        }
      ],
      "title": "P95 Response Time",
      "type": "stat"
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 5 },
      "id": 101,
      "title": "Galera Cluster",
      "type": "row"
    },
    {
      "datasource": "${DS_PROMETHEUS}",
      "fieldConfig": {
        "defaults": { "unit": "ops" }
      },
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 6 },
      "id": 10,
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      },
      "targets": [
        {
          "expr": "rate(mysql_global_status_wsrep_received[5m])",
          "legendFormat": "{{ instance }} received"
        },
        {
          "expr": "rate(mysql_global_status_wsrep_replicated[5m])",
          "legendFormat": "{{ instance }} replicated"
        }
      ],
      "title": "Galera Replication Rate",
      "type": "timeseries"
    },
    {
      "datasource": "${DS_PROMETHEUS}",
      "fieldConfig": {
        "defaults": { "unit": "short" }
      },
      "gridPos": { "h": 8, "w": 8, "x": 8, "y": 6 },
      "id": 11,
      "targets": [
        {
          "expr": "mysql_global_status_wsrep_local_recv_queue_avg",
          "legendFormat": "{{ instance }} recv queue avg"
        },
        {
          "expr": "mysql_global_status_wsrep_local_send_queue_avg",
          "legendFormat": "{{ instance }} send queue avg"
        }
      ],
      "title": "Galera Queue Sizes",
      "type": "timeseries"
    },
    {
      "datasource": "${DS_PROMETHEUS}",
      "fieldConfig": {
        "defaults": { "unit": "percentunit" }
      },
      "gridPos": { "h": 8, "w": 8, "x": 16, "y": 6 },
      "id": 12,
      "targets": [
        {
          "expr": "rate(mysql_global_status_wsrep_flow_control_paused_ns[5m]) / 1e9",
          "legendFormat": "{{ instance }} flow control paused"
        }
      ],
      "title": "Galera Flow Control",
      "type": "timeseries"
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 14 },
      "id": 102,
      "title": "HAProxy",
      "type": "row"
    },
    {
      "datasource": "${DS_PROMETHEUS}",
      "fieldConfig": {
        "defaults": { "unit": "reqps" }
      },
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 15 },
      "id": 20,
      "targets": [
        {
          "expr": "sum(rate(haproxy_backend_http_responses_total{backend=\"drupal_servers\"}[5m])) by (code)",
          "legendFormat": "{{ code }}"
        }
      ],
      "title": "HAProxy Request Rate by Status",
      "type": "timeseries"
    },
    {
      "datasource": "${DS_PROMETHEUS}",
      "fieldConfig": {
        "defaults": { "unit": "s" }
      },
      "gridPos": { "h": 8, "w": 8, "x": 8, "y": 15 },
      "id": 21,
      "targets": [
        {
          "expr": "haproxy_backend_http_response_time_average_seconds{backend=\"drupal_servers\"}",
          "legendFormat": "Average"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket{job=\"drupal\"}[5m])) by (le))",
          "legendFormat": "P95"
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(http_request_duration_seconds_bucket{job=\"drupal\"}[5m])) by (le))",
          "legendFormat": "P99"
        }
      ],
      "title": "Response Time (Avg / P95 / P99)",
      "type": "timeseries"
    },
    {
      "datasource": "${DS_PROMETHEUS}",
      "fieldConfig": {
        "defaults": { "unit": "short" }
      },
      "gridPos": { "h": 8, "w": 8, "x": 16, "y": 15 },
      "id": 22,
      "targets": [
        {
          "expr": "haproxy_backend_current_sessions{backend=\"drupal_servers\"}",
          "legendFormat": "Active Sessions"
        },
        {
          "expr": "haproxy_backend_current_queue{backend=\"drupal_servers\"}",
          "legendFormat": "Queue"
        }
      ],
      "title": "HAProxy Sessions & Queue",
      "type": "timeseries"
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 23 },
      "id": 103,
      "title": "ProxySQL",
      "type": "row"
    },
    {
      "datasource": "${DS_PROMETHEUS}",
      "fieldConfig": {
        "defaults": { "unit": "short" }
      },
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 24 },
      "id": 30,
      "targets": [
        {
          "expr": "proxysql_connection_pool_conn_used",
          "legendFormat": "{{ hostgroup }} used"
        },
        {
          "expr": "proxysql_connection_pool_conn_free",
          "legendFormat": "{{ hostgroup }} free"
        }
      ],
      "title": "ProxySQL Connection Pool",
      "type": "timeseries"
    },
    {
      "datasource": "${DS_PROMETHEUS}",
      "fieldConfig": {
        "defaults": { "unit": "ops" }
      },
      "gridPos": { "h": 8, "w": 8, "x": 8, "y": 24 },
      "id": 31,
      "targets": [
        {
          "expr": "rate(proxysql_command_counter_total{command=\"select\"}[5m])",
          "legendFormat": "SELECT/s"
        },
        {
          "expr": "rate(proxysql_command_counter_total{command=\"insert\"}[5m])",
          "legendFormat": "INSERT/s"
        },
        {
          "expr": "rate(proxysql_command_counter_total{command=\"update\"}[5m])",
          "legendFormat": "UPDATE/s"
        },
        {
          "expr": "rate(proxysql_command_counter_total{command=\"delete\"}[5m])",
          "legendFormat": "DELETE/s"
        }
      ],
      "title": "ProxySQL Query Rate by Type",
      "type": "timeseries"
    },
    {
      "datasource": "${DS_PROMETHEUS}",
      "fieldConfig": {
        "defaults": { "unit": "ms" }
      },
      "gridPos": { "h": 8, "w": 8, "x": 16, "y": 24 },
      "id": 32,
      "targets": [
        {
          "expr": "proxysql_connection_pool_latency_ms",
          "legendFormat": "{{ hostgroup }}/{{ endpoint }}"
        }
      ],
      "title": "ProxySQL Backend Latency",
      "type": "timeseries"
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 32 },
      "id": 104,
      "title": "Redis",
      "type": "row"
    },
    {
      "datasource": "${DS_PROMETHEUS}",
      "fieldConfig": {
        "defaults": { "unit": "bytes" }
      },
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 33 },
      "id": 40,
      "targets": [
        {
          "expr": "redis_memory_used_bytes",
          "legendFormat": "{{ instance }} used"
        },
        {
          "expr": "redis_memory_max_bytes",
          "legendFormat": "{{ instance }} max"
        }
      ],
      "title": "Redis Memory Usage",
      "type": "timeseries"
    },
    {
      "datasource": "${DS_PROMETHEUS}",
      "fieldConfig": {
        "defaults": { "unit": "ops" }
      },
      "gridPos": { "h": 8, "w": 8, "x": 8, "y": 33 },
      "id": 41,
      "targets": [
        {
          "expr": "rate(redis_commands_processed_total[5m])",
          "legendFormat": "{{ instance }}"
        }
      ],
      "title": "Redis Commands/sec",
      "type": "timeseries"
    },
    {
      "datasource": "${DS_PROMETHEUS}",
      "fieldConfig": {
        "defaults": { "unit": "short" }
      },
      "gridPos": { "h": 8, "w": 8, "x": 16, "y": 33 },
      "id": 42,
      "targets": [
        {
          "expr": "redis_connected_clients",
          "legendFormat": "{{ instance }} clients"
        },
        {
          "expr": "redis_keyspace_hits_total / (redis_keyspace_hits_total + redis_keyspace_misses_total) * 100",
          "legendFormat": "{{ instance }} hit rate %"
        }
      ],
      "title": "Redis Clients & Hit Rate",
      "type": "timeseries"
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 41 },
      "id": 105,
      "title": "Infrastructure",
      "type": "row"
    },
    {
      "datasource": "${DS_PROMETHEUS}",
      "fieldConfig": {
        "defaults": { "unit": "percentunit" }
      },
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 42 },
      "id": 50,
      "targets": [
        {
          "expr": "1 - (node_filesystem_avail_bytes{mountpoint=\"/\"} / node_filesystem_size_bytes{mountpoint=\"/\"})",
          "legendFormat": "{{ instance }}"
        }
      ],
      "title": "Disk Usage (/)",
      "type": "timeseries"
    },
    {
      "datasource": "${DS_PROMETHEUS}",
      "fieldConfig": {
        "defaults": { "unit": "percentunit" }
      },
      "gridPos": { "h": 8, "w": 8, "x": 8, "y": 42 },
      "id": 51,
      "targets": [
        {
          "expr": "1 - avg by(instance) (rate(node_cpu_seconds_total{mode=\"idle\"}[5m]))",
          "legendFormat": "{{ instance }}"
        }
      ],
      "title": "CPU Usage",
      "type": "timeseries"
    },
    {
      "datasource": "${DS_PROMETHEUS}",
      "fieldConfig": {
        "defaults": { "unit": "percentunit" }
      },
      "gridPos": { "h": 8, "w": 8, "x": 16, "y": 42 },
      "id": 52,
      "targets": [
        {
          "expr": "1 - (node_memory_MemAvailable_bytes / node_memory_MemTotal_bytes)",
          "legendFormat": "{{ instance }}"
        }
      ],
      "title": "Memory Usage",
      "type": "timeseries"
    }
  ],
  "refresh": "30s",
  "schemaVersion": 36,
  "style": "dark",
  "tags": ["jaraba", "ha", "infrastructure", "n3-enterprise"],
  "templating": {
    "list": []
  },
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": ["5s", "10s", "30s", "1m", "5m"]
  },
  "timezone": "Europe/Madrid",
  "title": "Jaraba HA Stack - N3 Enterprise",
  "uid": "jaraba-ha-n3",
  "version": 1
}
